      "$id": 68,
      "label": "qev092003_JNK0040",
      "style": 4,
      "name": "자수"
    },
    {
      "$id": 69,
      "label": "qev092003_SEL0041",
      "style": 3,
      "name": "범인은 자신이었다며 자수한다"
    },
    {
      "$id": 70,
      "label": "qev092003_MSG0057",
      "style": 0,
      "name": "그래, 그렇게 하자![ST:wait ]"
