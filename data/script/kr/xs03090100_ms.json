  "rows": [
    {
      "$id": 1,
      "label": "xs03090100_MSG0001",
      "style": 0,
      "name": "어때? 블레이드 일은 조금 익숙해졌어?[ST:wait ]"
    },
    {
      "$id": 2,
      "label": "xs03090100_JNK0010",
      "style": 4,
      "name": "여유"
    },
    {
      "$id": 3,
      "label": "xs03090100_SEL0011",
      "style": 3,
      "name": "자신만만하게 팔짱을 낀다"
    },
    {
      "$id": 4,
      "label": "xs03090100_JNK0020",
      "style": 4,
      "name": "불안"
    },
    {
      "$id": 5,
      "label": "xs03090100_SEL0021",
      "style": 3,
      "name": "어두운 얼굴로 고개를 젓는다"
    },
    {
      "$id": 6,
      "label": "xs03090100_MSG0002",
      "style": 0,
      "name": "역시 대단해요![ST:wait ]"
    },
    {
      "$id": 7,
      "label": "xs03090100_MSG0003",
      "style": 0,
      "name": "엘마 씨와 반담 사령관님이\n눈여겨봤다는 것도 납득이 가요[ST:wait ]"
    },
    {
      "$id": 8,
      "label": "xs03090100_MSG0004",
      "style": 0,
      "name": "하지만 블레이드에게 교만함은 금물이야\n만약을 위해 다시 한번 복습해 두자[ST:wait ]"
    },
    {
      "$id": 9,
      "label": "xs03090100_MSG0005",
      "style": 0,
      "name": "여러 일이 있었으니까요[ST:wait ]"
    },
    {
      "$id": 10,
      "label": "xs03090100_MSG0006",
      "style": 0,
      "name": "그래, 좋아\n여기서 다시 한번 복습해 두자[ST:wait ]"
    },
    {
      "$id": 11,
      "label": "xs03090100_MSG0007",
      "style": 0,
      "name": "우리 블레이드 최대의 임무는\n라이프 포인트의 탐색[ST:wait ]"
    },
    {
      "$id": 12,
      "label": "xs03090100_MSG0008",
      "style": 0,
      "name": "이건 알겠지?[ST:wait ]"
    },
    {
      "$id": 13,
      "label": "xs03090100_MSG0009",
      "style": 0,
      "name": "잃어버린 라이프에는\n아직 많은 사람이 잠들어 있어[ST:wait ]"
    },
    {
      "$id": 14,
      "label": "xs03090100_MSG0010",
      "style": 0,
      "name": "지금까지는 원시 생물에 의해 라이프가\n파괴되는 걸 걱정해 왔지만[ST:wait ]"
    },
    {
      "$id": 15,
      "label": "xs03090100_MSG0011",
      "style": 0,
      "name": "이번 임무에서 확인된\n인류를 적대시하는 지적 생명체의 출현은[ST:wait ]"
    },
    {
      "$id": 16,
      "label": "xs03090100_MSG0012",
      "style": 0,
      "name": "그 이상의 위협이 돼[ST:wait ]"
    },
    {
      "$id": 17,
      "label": "xs03090100_MSG0013",
      "style": 0,
      "name": "그러니까 조금이라도 빨리\n라이프를 찾아야만 해[ST:wait ]"
    },
    {
      "$id": 18,
      "label": "xs03090100_MSG0014",
      "style": 0,
      "name": "라이프를 찾기 위한 가장 빠른 길은\n프런티어넷의 확충이에요[ST:wait ]"
    },
    {
      "$id": 19,
      "label": "xs03090100_MSG0015",
      "style": 0,
      "name": "그리고 그 확충에 필요한 게\n광범위한 탐사가 가능한 데이터 프로브 설치예요[ST:wait ]"
    },
    {
      "$id": 20,
      "label": "xs03090100_MSG0016",
      "style": 0,
      "name": "즉, 블레이드 대원인 당신이\n최우선으로 해야 하는 일은[ST:wait ]"
    },
    {
      "$id": 21,
      "label": "xs03090100_MSG0017",
      "style": 0,
      "name": "미라 각지를 돌며 데이터 프로브를 설치해서\n프런티어넷을 넓히는 거야[ST:wait ]"
    },
    {
      "$id": 22,
      "label": "xs03090100_MSG0018",
      "style": 0,
      "name": "여기까지는 이해했어?[ST:wait ]"
    },
    {
      "$id": 23,
      "label": "xs03090100_JNK0030",
      "style": 4,
      "name": "상식"
    },
    {
      "$id": 24,
      "label": "xs03090100_SEL0031",
      "style": 3,
      "name": "괜찮다고 끄덕인다"
    },
    {
      "$id": 25,
      "label": "xs03090100_JNK0040",
      "style": 4,
      "name": "복습"
    },
    {
      "$id": 26,
      "label": "xs03090100_SEL0041",
      "style": 3,
      "name": "얼굴을 찡그리며 고개를 갸웃한다"
    },
    {
      "$id": 27,
      "label": "xs03090100_MSG0019",
      "style": 0,
      "name": "그렇다면 블레이드의\n또 다른 임무에 관해 설명할게[ST:wait ]"
    },
    {
      "$id": 28,
      "label": "xs03090100_MSG0020",
      "style": 0,
      "name": "그렇다면 다시 한번 가르쳐 줄게[ST:wait ]"
    },
    {
      "$id": 29,
      "label": "xs03090100_MSG0021",
      "style": 0,
      "name": "블레이드의 또 다른 임무\n그건 인류 존속을 위해[ST:wait ]"
    },
    {
      "$id": 30,
      "label": "xs03090100_MSG0022",
      "style": 0,
      "name": "위협이 되는 원시 생물을 토벌하는 거야[ST:wait ]"
    },
    {
      "$id": 31,
      "label": "xs03090100_MSG0023",
      "style": 0,
      "name": "행성 미라에 불시착한 우리는[ST:wait ]"
    },
    {
      "$id": 32,
      "label": "xs03090100_MSG0024",
      "style": 0,
      "name": "여기서 살아갈 것을\n각오해야만 해[ST:wait ]"
    },
    {
      "$id": 33,
      "label": "xs03090100_MSG0025",
      "style": 0,
      "name": "하지만 그러기엔 위험이 너무 많아\n안심하고 살기에는 아직 갈 길이 멀지[ST:wait ]"
    },
    {
      "$id": 34,
      "label": "xs03090100_MSG0026",
      "style": 0,
      "name": "우리 사정 때문에 원시 생물을 제거하는 건\n마음이 괴롭지만[ST:wait ]"
    },
    {
      "$id": 35,
      "label": "xs03090100_MSG0027",
      "style": 0,
      "name": "지구인이라는 종을 존속시키기 위해서는\n어쩔 수가 없어요[ST:wait ]"
    },
    {
      "$id": 36,
      "label": "xs03090100_MSG0028",
      "style": 0,
      "name": "하지만 모든 원시 생물을\n토벌하는 게 아니에요![ST:wait ]"
    },
    {
      "$id": 37,
      "label": "xs03090100_MSG0029",
      "style": 0,
      "name": "어디까지나 블레이드 본부가……[ST:wait ]"
    },
    {
      "$id": 38,
      "label": "xs03090100_MSG0030",
      "style": 0,
      "name": "인류 존망과 관련이 있다고 판단한\n원시 생물만을 토벌하는 거예요[ST:wait ]"
    },
    {
      "$id": 39,
      "label": "xs03090100_MSG0031",
      "style": 0,
      "name": "당신은 이 임무를 받기 위해\n스쿼드에 참가하게 될 거야[ST:wait ]"
    },
    {
      "$id": 40,
      "label": "xs03090100_MSG0032",
      "style": 0,
      "name": "그건 이 임무가 퀘스트 보드에서 수주하는\n일반 임무와는 다르게[ST:wait ]"
    },
    {
      "$id": 41,
      "label": "xs03090100_MSG0033",
      "style": 0,
      "name": "블레이드 본부에서 직접\n전 대원에게 내리는 임무이기 때문이야[ST:wait ]"
    },
    {
      "$id": 42,
      "label": "xs03090100_MSG0034",
      "style": 0,
      "name": "우리는 이 특별 임무를\n블레이드 퀘스트라고 부르고 있어[ST:wait ]"
    },
    {
      "$id": 43,
      "label": "xs03090100_MSG0035",
      "style": 0,
      "name": "스쿼드라는 건\n최대 32명이 소속되는 부대 같은 거예요[ST:wait ]"
    },
    {
      "$id": 44,
      "label": "xs03090100_MSG0036",
      "style": 0,
      "name": "전 대원에게 내려지는 미션은\n이례적이긴 하지만[ST:wait ]"
    },
    {
      "$id": 45,
      "label": "xs03090100_MSG0037",
      "style": 0,
      "name": "한두 명이서는 도저히 달성할 수 없는\n임무이기 때문이에요[ST:wait ]"
    },
    {
      "$id": 46,
      "label": "xs03090100_MSG0038",
      "style": 0,
      "name": "그러니까 앞으로 당신은 처음에……[ST:wait ]"
    },
    {
      "$id": 47,
      "label": "xs03090100_MSG0039",
      "style": 0,
      "name": "무조건 참가 스쿼드를 선택하게 될 테니\n그렇게 알고 있으면 돼[ST:wait ]"
    },
    {
      "$id": 48,
      "label": "xs03090100_MSG0040",
      "style": 0,
      "name": "그리고 한 가지 더[ST:wait ]"
    },
    {
      "$id": 49,
      "label": "xs03090100_MSG0041",
      "style": 0,
      "name": "이 블레이드 퀘스트는\n반드시 해야 하는 임무는 아니야[ST:wait ]"
    },
    {
      "$id": 50,
      "label": "xs03090100_MSG0042",
      "style": 0,
      "name": "대원에 따라서는 이 임무보다 긴급성이 높은\n일반 임무를 수행하는 경우도 있기 때문이야[ST:wait ]"
    },
    {
      "$id": 51,
      "label": "xs03090100_MSG0043",
      "style": 0,
      "name": "전 대원이 대상인 건\n그런 사태도 예상해서 그런 거야[ST:wait ]"
    },
    {
      "$id": 52,
      "label": "xs03090100_MSG0044",
      "style": 0,
      "name": "대략적인 복습과 설명은 끝났네요[ST:wait ]"
    },
    {
      "$id": 53,
      "label": "xs03090100_MSG0045",
      "style": 0,
      "name": "라이프 포인트 탐색을 위한[ST:wait ]"
    },
    {
      "$id": 54,
      "label": "xs03090100_MSG0045_2",
      "style": 0,
      "name": "데이터 프로브 설치와\n프런티어넷 확충[ST:wait ]"
    },
    {
      "$id": 55,
      "label": "xs03090100_MSG0046",
      "style": 0,
      "name": "그리고 이 행성에서 살아가기 위한\n블레이드 퀘스트[ST:wait ]"
    },
    {
      "$id": 56,
      "label": "xs03090100_MSG0047",
      "style": 0,
      "name": "이해했어?[ST:wait ]"
    },
    {
      "$id": 57,
      "label": "xs03090100_JNK0050",
      "style": 4,
      "name": "완벽"
    },
    {
      "$id": 58,
      "label": "xs03090100_SEL0051",
      "style": 3,
      "name": "문제없다며 강하게 끄덕인다"
    },
    {
      "$id": 59,
      "label": "xs03090100_JNK0060",
      "style": 4,
      "name": "혼란"
    },
    {
      "$id": 60,
      "label": "xs03090100_SEL0061",
      "style": 3,
      "name": "멍하니 먼 곳을 바라본다"
    },
    {
      "$id": 61,
      "label": "xs03090100_MSG0048",
      "style": 0,
      "name": "당신 실력에 기대하고 있어[ST:wait ]"
