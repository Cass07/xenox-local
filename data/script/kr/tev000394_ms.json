    {
      "$id": 2,
      "label": "tev000394_MSG0002",
      "style": 1,
      "name": "총을 점검하고 있었어\n우리는 B.B.지만 가만히 있으면\n실력이 둔해질 테니까[ST:wait ]"
    },
    {
      "$id": 3,
      "label": "tev000394_JNK0010",
      "style": 4,
      "name": "제안"
    },
    {
      "$id": 4,
      "label": "tev000394_SEL0011",
      "style": 3,
      "name": "신고와 함께\n특훈을 하라고 권한다"
    },
    {
      "$id": 5,
      "label": "tev000394_JNK0020",
      "style": 4,
      "name": "질문"
    },
    {
      "$id": 6,
      "label": "tev000394_SEL0021",
      "style": 3,
      "name": "요즘 임무에 나타나지 않는\n이유를 묻는다"
    },
    {
      "$id": 7,
      "label": "tev000394_MSG0003",
      "style": 1,
      "name": "신고를 가르치면서\n나도 배울 수 있는 게 많겠지만……[ST:wait ]"
    },
    {
      "$id": 8,
      "label": "tev000394_MSG0004",
      "style": 1,
      "name": "솔직히 말해서 시간이 없거든……[ST:wait ]"
    },
    {
      "$id": 9,
      "label": "tev000394_MSG0005",
      "style": 1,
      "name": "게다가 그 녀석과 난 스타일이 전혀 달라\n전문가한테 따로 배우는 편이 녀석한테도 이로울 거다[ST:wait ]"
    },
    {
      "$id": 10,
      "label": "tev000394_MSG0006",
      "style": 1,
      "name": "……이러면 변명처럼 들리려나[ST:wait ]"
    },
    {
      "$id": 11,
      "label": "tev000394_MSG0007",
      "style": 1,
      "name": "누구든 해야만 하는 일이 있지[ST:wait ]"
    },
    {
      "$id": 12,
      "label": "tev000394_MSG0008",
      "style": 1,
      "name": "넌 목숨을 걸어야만 하는 임무에\n린이나 타츠를 데려갈 거냐?[ST:wait ]"
    },
    {
      "$id": 13,
      "label": "tev000394_MSG0009",
      "style": 1,
      "name": "두고 갈 거잖아?\n나도 똑같아[ST:wait ]"
    },
    {
      "$id": 14,
      "label": "tev000394_MSG0010",
      "style": 1,
      "name": "지금은 자세히 말 못 하지만\n[ST:playername ][KR:Particle char=to ] 엘마가 그렇듯이\n나도 이루어야 할 목표가 있다고[ST:wait ]"
    },
    {
      "$id": 15,
      "label": "tev000394_JNK0030",
      "style": 4,
      "name": "반론한다"
    },
    {
      "$id": 16,
      "label": "tev000394_SEL0031",
      "style": 3,
      "name": "그래도 팀 멤버에게 관심을\n가지는 것은 중요하다고 말한다"
    },
    {
      "$id": 17,
      "label": "tev000394_JNK0040",
      "style": 4,
      "name": "응원한다"
    },
    {
      "$id": 18,
      "label": "tev000394_SEL0041",
      "style": 3,
      "name": "라오를 응원한다"
    },
    {
      "$id": 19,
      "label": "tev000394_MSG0011",
      "style": 1,
      "name": "훗……[ST:wait ]"
    },
    {
      "$id": 20,
      "label": "tev000394_MSG0012",
      "style": 1,
      "name": "더그랑 똑같은 소릴 하는군[ST:wait ]"
    },
    {
      "$id": 21,
      "label": "tev000394_MSG0013",
      "style": 1,
      "name": "나도 알아, 자이덴도 몬도도 신고도\n믿을 수 있는 소중한 동료들이야[ST:wait ]"
    },
    {
      "$id": 22,
      "label": "tev000394_MSG0014",
      "style": 1,
      "name": "절대로 녀석들을 소홀히 할 생각은 없어[ST:wait ]"
    },
    {
      "$id": 23,
      "label": "tev000394_MSG0015",
      "style": 1,
      "name": "지금까지는 대화가 부족했던 것 같으니\n내 이야기도 조금은 해 볼게[ST:wait ]"
    },
    {
      "$id": 24,
      "label": "tev000394_MSG0016",
      "style": 1,
      "name": "……이제 만족하냐?[ST:wait ]"
    },
    {
      "$id": 25,
      "label": "tev000394_MSG0017",
      "style": 1,
      "name": "하여튼 [ST:playername ]도\n참견쟁이라니까[ST:wait ]"
    },
    {
      "$id": 26,
      "label": "tev000394_MSG0018",
      "style": 1,
      "name": "자, 난 슬슬 가 봐야겠다\n자이덴과 만나기로 한 시간이거든[ST:wait ]"
    },
    {
      "$id": 27,
      "label": "tev000394_MSG0019",
      "style": 1,
      "name": "그럼 또 보자[ST:wait ]"
    },
    {
      "$id": 28,
      "label": "tev000394_MSG0020",
      "style": 1,
      "name": "아무 말 없이 그저 날 지켜봐 주겠다는 건가[ST:wait ]"
    },
    {
      "$id": 29,
      "label": "tev000394_MSG0021",
      "style": 1,
      "name": "고맙다, 날 믿어 줘서[ST:wait ]"
    },
    {
      "$id": 30,
      "label": "tev000394_MSG0022",
      "style": 1,
      "name": "그렇지……\n나중에 신고와 대련을 해 줘[ST:wait ]"
    },
    {
      "$id": 31,
      "label": "tev000394_MSG0023",
      "style": 1,
      "name": "너 정도 실력이라면\n녀석에게도 좋은 자극이 될 거고\n배울 점도 있을 테니까 말이야[ST:wait ]"
    },
    {
      "$id": 32,
      "label": "tev000394_MSG0024",
      "style": 1,
      "name": "잘 부탁한다……[ST:wait ]"
    }
  ]
