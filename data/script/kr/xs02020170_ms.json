  "rows": [
    {
      "$id": 1,
      "label": "xs02020170_MSG0001",
      "style": 0,
      "name": "이건…… 무슨……[ST:wait ]"
    },
    {
      "$id": 2,
      "label": "xs02020170_MSG0002",
      "style": 0,
      "name": "저 버설트 하나가\n이만큼의 시미우스를 쓰러뜨린 걸까요?[ST:wait ]"
    },
    {
      "$id": 3,
      "label": "xs02020170_MSG0003",
      "style": 0,
      "name": "설마……[ST:wait ]"
    },
    {
      "$id": 4,
      "label": "xs02020170_MSG0004",
      "style": 0,
      "name": "엘마…… 씨?[ST:wait ]"
    },
    {
      "$id": 5,
      "label": "xs02020170_MSG0005",
      "style": 0,
      "name": "상황이 안 좋네[ST:wait ]"
    },
    {
      "$id": 6,
      "label": "xs02020170_MSG0006",
      "style": 0,
      "name": "무슨 말인가요?[ST:wait ]"
    },
    {
      "$id": 7,
      "label": "xs02020170_MSG0007",
      "style": 0,
      "name": "저 버설트…… 오버드야[ST:wait ]"
    },
    {
      "$id": 8,
      "label": "xs02020170_MSG0008",
      "style": 0,
      "name": "네!?[ST:wait ]"
    },
    {
      "$id": 9,
      "label": "xs02020170_MSG0009",
      "style": 0,
      "name": "저는 평범한 버설트로 보이는데요……[ST:wait ]"
    },
    {
      "$id": 10,
      "label": "xs02020170_MSG0010",
      "style": 0,
      "name": "한 마리의 시미우스가 3, 4마리의 버설트를\n쓰러뜨리는 건 충분히 있을 수 있는 일이야[ST:wait ]"
    },
    {
      "$id": 11,
      "label": "xs02020170_MSG0011",
      "style": 0,
      "name": "하지만 지금 눈앞에 있는 광경은 그 반대지\n다른 버설트가 있었던 흔적도 없어[ST:wait ]"
    },
    {
      "$id": 12,
      "label": "xs02020170_MSG0012",
      "style": 0,
      "name": "그렇다면 이 상황을 설명할 수 있는 답은[ST:wait ]"
    },
    {
      "$id": 13,
      "label": "xs02020170_MSG0013",
      "style": 0,
      "name": "저 버설트가 오버드라는 것뿐이야[ST:wait ]"
    },
    {
      "$id": 14,
      "label": "xs02020170_MSG0014",
      "style": 0,
      "name": "블레이드 리포트 데이터베이스로\n알아볼게요![ST:wait ]"
    },
    {
      "$id": 15,
      "label": "xs02020170_MSG0015",
      "style": 0,
      "name": "저 버설트가 오버드라면\n분명 뭔가 나와 있을 거예요[ST:wait ]"
    },
    {
      "$id": 16,
      "label": "xs02020170_MSG0016",
      "style": 0,
      "name": "있어요![ST:wait ]"
    },
    {
      "$id": 17,
      "label": "xs02020170_MSG0017",
      "style": 0,
      "name": "하지만 여기서 꽤 떨어진 에어리어에서\n활동한다고 보고되어 있어요[ST:wait ]"
    },
    {
      "$id": 18,
      "label": "xs02020170_MSG0018",
      "style": 0,
      "name": "도망친 블레이드 대원을 쫓아 여기까지 왔거나\n아예 다른 이유가 있을 수도[ST:wait ]"
    },
    {
      "$id": 19,
      "label": "xs02020170_MSG0019",
      "style": 0,
      "name": "오버드가 이렇게 가까이 있는데\n블레이드 본부는 아무런 움직임이 없네요[ST:wait ]"
    },
    {
      "$id": 20,
      "label": "xs02020170_MSG0020",
      "style": 0,
      "name": "데이터 프로브가\n설치되어 있지 않은 장소의 정보는……[ST:wait ]"
    },
    {
      "$id": 21,
      "label": "xs02020170_MSG0021",
      "style": 0,
      "name": "프런티어넷으로는 확인할 수 없어[ST:wait ]"
    },
    {
      "$id": 22,
      "label": "xs02020170_MSG0022",
      "style": 0,
      "name": "감시하고 있던 데이터 프로브의\n세그먼트에서 벗어나서……[ST:wait ]"
    },
    {
      "$id": 23,
      "label": "xs02020170_MSG0023",
      "style": 0,
      "name": "놓쳐 버린 거군요[ST:wait ]"
    },
    {
      "$id": 24,
      "label": "xs02020170_JNK0010",
      "style": 4,
      "name": "강건"
    },
    {
      "$id": 25,
      "label": "xs02020170_SEL0011",
      "style": 3,
      "name": "싸울 자세를 보인다"
    },
    {
      "$id": 26,
      "label": "xs02020170_JNK0020",
      "style": 4,
      "name": "의문"
    },
    {
      "$id": 27,
      "label": "xs02020170_SEL0021",
      "style": 3,
      "name": "오버드가 무엇인지 묻는다"
    },
    {
      "$id": 28,
      "label": "xs02020170_MSG0024",
      "style": 0,
      "name": "자, 자, 자, 자, 잠깐 기다려 주세요![ST:wait ]"
    },
    {
      "$id": 29,
      "label": "xs02020170_MSG0025",
      "style": 0,
      "name": "진정하세요!\n상대는 오버드라고요![ST:wait ]"
    },
    {
      "$id": 30,
      "label": "xs02020170_MSG0026",
      "style": 0,
      "name": "아직 알려 주지 않았지?[ST:wait ]"
    },
    {
      "$id": 31,
      "label": "xs02020170_MSG0027",
      "style": 0,
      "name": "오버드라는 건 NLA에 위협적인 대상인\n원시 생물 중에서도 랭크 밖……[ST:wait ]"
    },
    {
      "$id": 32,
      "label": "xs02020170_MSG0028",
      "style": 0,
      "name": "즉, 랭크를 붙일 수 없을 정도로\n강한 위협이 될 수 있는 원시 생물의 총칭이야[ST:wait ]"
    },
    {
      "$id": 33,
      "label": "xs02020170_MSG0029",
      "style": 0,
      "name": "다만, 단순히 강하다는 이유만으로\n지정되는 것이 아니라[ST:wait ]"
    },
    {
      "$id": 34,
      "label": "xs02020170_MSG0030",
      "style": 0,
      "name": "무언가 하나라도 위협적인 능력이 있으면\n그 대상이 되는 거예요[ST:wait ]"
    },
    {
      "$id": 35,
      "label": "xs02020170_MSG0031",
      "style": 0,
      "name": "오버드라는 건……[ST:wait ]"
    },
    {
      "$id": 36,
      "label": "xs02020170_MSG0032",
      "style": 0,
      "name": "미라에 사는 원시 생물 중에서도\n인류에게 위협이 되는 원시 생물을 말해요[ST:wait ]"
    },
    {
      "$id": 37,
      "label": "xs02020170_MSG0033",
      "style": 0,
      "name": "실제로 싸워 보니 저랭크였던 일도\n있었다고는 하지만요[ST:wait ]"
    },
    {
      "$id": 38,
      "label": "xs02020170_MSG0034",
      "style": 0,
      "name": "하지만 오해하지 마[ST:wait ]"
    },
    {
      "$id": 39,
      "label": "xs02020170_MSG0035",
      "style": 0,
      "name": "오버드로 지정된 원시 생물은\n단순히 강하다고 해서 선택된 게 아니야[ST:wait ]"
    },
    {
      "$id": 40,
      "label": "xs02020170_MSG0036",
      "style": 0,
      "name": "하나라도 인류의 위협이 될 수 있는 능력을\n가지고 있으면 그 대상이 되는 거야[ST:wait ]"
    },
    {
      "$id": 41,
      "label": "xs02020170_MSG0037",
      "style": 0,
      "name": "블레이드에서도 1, 2위를 다투는 실력 있는 팀이라도\n여러 명의 사상자를 내는 원시 생물[ST:wait ]"
    },
    {
      "$id": 42,
      "label": "xs02020170_MSG0038",
      "style": 0,
      "name": "그게 오버드야[ST:wait ]"
    },
    {
      "$id": 43,
      "label": "xs02020170_JNK0030",
      "style": 4,
      "name": "신뢰"
    },
    {
      "$id": 44,
      "label": "xs02020170_SEL0031",
      "style": 3,
      "name": "린과 엘마를 보며 미소 짓는다"
    },
    {
      "$id": 45,
      "label": "xs02020170_JNK0040",
      "style": 4,
      "name": "신념"
    },
    {
      "$id": 46,
      "label": "xs02020170_SEL0041",
      "style": 3,
      "name": "NLA를 바라보며 주먹을 강하게 쥔다"
    },
    {
      "$id": 47,
      "label": "xs02020170_MSG0039",
      "style": 0,
      "name": "[ST:undisp ][ST:wide ]―[ST:wait ]"
    },
    {
      "$id": 48,
      "label": "xs02020170_MSG0040",
      "style": 0,
      "name": "저 같은 사람을 믿어 주시다니\n감사합니다[ST:wait ]"
    },
    {
      "$id": 49,
      "label": "xs02020170_MSG0041",
      "style": 0,
      "name": "엘마 씨, 저 하겠어요![ST:wait ]"
    },
    {
      "$id": 50,
      "label": "xs02020170_MSG0042",
      "style": 0,
      "name": "앞으로 여기서 살아가는 이상[ST:wait ]"
    },
    {
      "$id": 51,
      "label": "xs02020170_MSG0043",
      "style": 0,
      "name": "오버드와 만나게 되는 일은\n몇 번이고 있을 거예요[ST:wait ]"
    },
    {
      "$id": 52,
      "label": "xs02020170_MSG0044",
      "style": 0,
      "name": "그때마다 도망칠 수는 없으니까요[ST:wait ]"
    },
    {
      "$id": 53,
      "label": "xs02020170_MSG0045",
      "style": 0,
      "name": "그리고 지금 도망쳐서 NLA에 피해가 생기면\n분명 후회할 거예요[ST:wait ]"
    },
    {
      "$id": 54,
      "label": "xs02020170_MSG0046",
      "style": 0,
      "name": "그렇네, 하자[ST:wait ]"
    },
    {
      "$id": 55,
      "label": "xs02020170_MSG0047",
      "style": 0,
      "name": "잘 생각해 본 후에 내린 결정이지?[ST:wait ]"
    },
    {
      "$id": 56,
      "label": "xs02020170_MSG0048",
      "style": 0,
      "name": "린[ST:wait ]"
    },
    {
      "$id": 57,
      "label": "xs02020170_MSG0049",
      "style": 0,
      "name": "저도 하겠어요![ST:wait ]"
    },
    {
      "$id": 58,
      "label": "xs02020170_MSG0050",
      "style": 0,
      "name": "NLA도 그렇지만[ST:wait ]"
    },
    {
      "$id": 59,
      "label": "xs02020170_MSG0051",
      "style": 0,
      "name": "앞으로 여기서 살아가는 이상\n오버드와의 싸움은 피할 수 없으니까요[ST:wait ]"
    },
    {
      "$id": 60,
      "label": "xs02020170_MSG0052",
      "style": 0,
      "name": "그리고 왠지 두 분과 함께라면\n쓰러뜨릴 수 있을 것 같은 기분이 들어요![ST:wait ]"
    },
    {
      "$id": 61,
      "label": "xs02020170_MSG0053",
      "style": 0,
      "name": "후훗, 그렇네[ST:wait ]"
    },
    {
      "$id": 62,
      "label": "xs02020170_MSG0054",
      "style": 0,
      "name": "둘 다 무리는 하지 마[ST:wait ]"
    },
    {
      "$id": 63,
      "label": "xs02020170_MSG0055",
      "style": 0,
      "name": "쓰러뜨리는 게 제일 좋겠지만\n일단 물러나게 하는 것만으로도 충분해[ST:wait ]"
    },
    {
      "$id": 64,
      "label": "xs02020170_MSG0056",
      "style": 0,
      "name": "그사이에 데이터 프로브를 설치하면[ST:wait ]"
    },
    {
      "$id": 65,
      "label": "xs02020170_MSG0057",
      "style": 0,
      "name": "저 녀석이 다시 NLA에 접근해 와도\n다음엔 프런티어넷으로 포착할 수 있어[ST:wait ]"
    },
    {
      "$id": 66,
      "label": "xs02020170_MSG0058",
      "style": 0,
      "name": "오버드는 지금까지의 원시 생물과는 달라\n단단히 준비하자[ST:wait ]"
    }
  ]
}